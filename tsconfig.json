    "**/*.ts",
    "**/*.tsx"
, "src/app/components/FormInstallPool/FormInstallPool.jsx", "src/app/page.jsx", "src/app/components/layouts/BannerCarousel/CarouselBanner.jsx", "src/app/components/layouts/BannerCarousel/CarouselBanner.style.jsx", "src/app/components/layouts/BannerCarousel/CustomCarousel.style.jsx", "src/app/components/layouts/BannerCarousel/CustomCarousel.jsx", "src/app/components/ProductList/ProductList.style.jsx", "src/app/components/ProductList/ProductList.component.jsx", "src/app/modelos/page.jsx"  ],
